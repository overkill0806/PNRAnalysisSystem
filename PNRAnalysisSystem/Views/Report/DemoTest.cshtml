</table>


<script>
    function testajax(){
        $.ajax({
            type: "POST",
            data: { str:"" },
            url: "/Report/GetTest",
            success: function (res) {
                alert(res.msg);
            },
            error: function () {
                alert('Data Server Busy');
            }
        });
    }
</script>

