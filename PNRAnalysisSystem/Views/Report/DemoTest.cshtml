@{
    Layout = null;
}
<link href="css/common-com.css" rel="stylesheet">
<script src="js/jquery.table2excel.js"></script>


<h2>Model Example</h2>

<button class="btn btn-success btn-icon-split" id="myBtn2" onclick="testajax()"> <span class="text">APITest</span></button>

<button class="btn btn-success btn-icon-split" id="myBtn3" onclick="createCsvFile('test',getRandomData())"><span class="text">CSVExport</span></button>

<button class="btn btn-success btn-icon-split" id="myBtn4" onclick="createTxtFile('test',getRandomData())"><span class="text">TxtExport</span></button>

<button class="btn btn-success btn-icon-split" id="myBtn5" onclick="createXmlFile('test','<sometag><someothertag></someothertag></sometag>')"><span class=" text">XmlExport</span></button>

<button class="btn btn-success btn-icon-split" id="myBtn6" onclick="createXlsFile('test','tblCustomers')"><span class="text">ExcelExport</span></button>

