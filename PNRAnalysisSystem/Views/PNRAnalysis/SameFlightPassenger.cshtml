<script>
    $(document).ready(function () {
        var controls = {
            leftArrow: '<i class="fal fa-angle-left" style="font-size: 1.25rem"></i>',
            rightArrow: '<i class="fal fa-angle-right" style="font-size: 1.25rem"></i>'
        }
        $('input[name=datePicker]').datepicker({
            orientation: "bottom right",
            todayHighlight: true,
            templates: controls,
            format: "yyyy/mm/dd"
        });
    });
    function process() {
        var flightno = $('[name=flightno]').val();
        LoadTable('SFlightPassenger_List', 'PNRAnalysis/ListSameFlightPassenger?flightno=' + flightno);
    }
    function goback() {
        $('#SFlightPassenger_List').show();
        $('#ListSameFlightPassengerDt').show();
        $('#SFlightPassengermain').show();
        $('#btnNewBatch').show();
        $('#SFlightPassengerView').hide();
        $('#btnGoBack').hide();
    }
</script>